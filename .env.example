# Supabase
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
NEXT_PUBLIC_DEV_SUPABASE_REDIRECT_URL=http://localhost:3000/auth/callback

# Stripe API Keys
STRIPE_SECRET_KEY=sk_test_...
STRIPE_PUBLISHABLE_KEY=pk_test_...
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_...

# Stripe Webhook Secret
STRIPE_WEBHOOK_SECRET=whsec_...

# Stripe Price IDs (from Stripe Dashboard)
STRIPE_PRICE_PRO_ID=price_...
STRIPE_PRICE_ENTERPRISE_ID=price_...

# Optional: Stripe Product IDs
STRIPE_PRODUCT_PRO_ID=prod_...
STRIPE_PRODUCT_ENTERPRISE_ID=prod_...

# App URLs
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_VERCEL_URL=http://localhost:3000

# Google Sheets (if using)
GOOGLE_CREDENTIALS_PATH=/path/to/credentials.json
GOOGLE_SHEETS_CUSTOMER_CONFIG_ID=spreadsheet_id
GOOGLE_SHEETS_ARTICLES_ID=spreadsheet_id
